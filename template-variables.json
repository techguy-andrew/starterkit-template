{
  "project": {
    "name": "nextjs-starter-kit",
    "description": "A modern Next.js starter kit with authentication, dashboard, and UI components",
    "version": "1.0.0",
    "author": "{{AUTHOR_NAME}}",
    "license": "MIT"
  },
  "features": {
    "authentication": true,
    "dashboard": true,
    "marketing": true,
    "api": true,
    "database": false,
    "payments": false,
    "email": false
  },
  "styling": {
    "primaryColor": "zinc",
    "accentColor": "blue",
    "theme": "light",
    "font": "inter"
  },
  "seo": {
