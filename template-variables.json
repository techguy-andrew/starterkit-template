    "defaultDescription": "{{PROJECT_DESCRIPTION}}",
    "keywords": ["nextjs", "react", "typescript", "tailwindcss"],
    "ogImage": "/og-image.png"
  },
  "site": {
    "url": "{{SITE_URL}}",
    "twitter": "{{TWITTER_URL}}",
    "github": "{{GITHUB_URL}}",
    "linkedin": "{{LINKEDIN_URL}}",
    "youtube": "{{YOUTUBE_URL}}",
    "contact": {
      "email": "{{CONTACT_EMAIL}}",
      "phone": "{{CONTACT_PHONE}}"
    }
  }
}
